title: "Royfactory"
description: "개발, AI, Kubernetes, 백엔드 기술 블로그"
url: "https://royfactory.github.io"
baseurl: ""
lang: "ko"

theme: minima
plugins:
  - jekyll-feed
  - jekyll-seo-tag
  - jekyll-paginate
  - jekyll-archives
  - jekyll-toc

paginate: 10
paginate_path: "/page:num/"

author:
  name: "Roy"
  email: "worry1318@naver.com"

social:
  name: "Royfactory Blog"
  links:
    - "https://github.com/royfactory"
    - "https://royzero.tistory.com/"

sitemap:
  priority: 
    "index.html": 1.0
    "/": 1.0
  changefreq:
    "index.html": "daily"
    "/": "daily"
  exclude:
    - /assets/
    - /_sass/
    - /vendor/
    - "*.json"
    - "*.txt"
    - "*.py"
